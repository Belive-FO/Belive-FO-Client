# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Laravel Backend API
NEXT_PUBLIC_LARAVEL_API_URL=http://localhost:8000
# Laravel session cookie name (match backend config/session.php SESSION_COOKIE or APP_NAME)
NEXT_PUBLIC_LARAVEL_SESSION_COOKIE=laravel-session

# Internal BFF Secret
BFF_INTERNAL_SECRET=your_internal_secret

# Lark OAuth Configuration
NEXT_PUBLIC_LARK_APP_ID=your_lark_app_id
NEXT_PUBLIC_LARK_REDIRECT_URI=http://localhost:3000/auth/callback
